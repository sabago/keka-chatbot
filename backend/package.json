{
  "name": "keka-chatbot-backend",
  "version": "1.0.0",
  "description": "Backend API for Keka chatbot",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && npm run copy:sql && npm run build:frontend",
    "copy:sql": "mkdir -p dist/db && cp src/db/*.sql dist/db/",
    "build:frontend": "cd ../frontend && npm run build && mkdir -p ../backend/dist/public && cp -r dist/* ../backend/dist/public/",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.917.0",
    "@aws-sdk/client-sns": "^3.936.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "pg": "^8.11.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.10.9",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
